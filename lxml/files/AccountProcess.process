<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:ns="http://www.tibco.com/namespaces/tnt/plugins/json" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions" xmlns:pfx="http://www.tibco.com/schemas/TIBCO_BW_REST_EXAMPLE/Schemas/Schema.xsd">
    <xsd:import namespace="http://www.tibco.com/schemas/TIBCO_BW_REST_EXAMPLE/Schemas/Schema.xsd" schemaLocation="/Schemas/AccountAttributes.xsd"/>
    <pd:name>Processes/AccountProcess.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType>
        <xsd:element name="InvokeProcessInput">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="FixedData" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:startType>
    <pd:startX>47</pd:startX>
    <pd:startY>62</pd:startY>
    <pd:returnBindings>
        <Response>
            <Result>
                <xsl:value-of select="tib:substring-before-last(substring-after($Render-JSON/ns:ActivityOutputClass/jsonString, '&quot;root&quot;:'), '}')"/>
            </Result>
        </Response>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType>
        <xsd:element name="Response">
            <xsd:complexType>
                <xsd:sequence>
                    <xsd:element name="Result" type="xsd:string"/>
                </xsd:sequence>
            </xsd:complexType>
        </xsd:element>
    </pd:endType>
    <pd:endX>644</pd:endX>
    <pd:endY>62</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1357991683487</pd:targetNamespace>
    <pd:activity name="GetDataFormat">
        <pd:type>com.tibco.pe.core.CallProcessActivity</pd:type>
        <pd:resourceType>ae.process.subprocess</pd:resourceType>
        <pd:x>166</pd:x>
        <pd:y>64</pd:y>
        <config>
            <processName>/Processes/GetDataFormat.process</processName>
        </config>
        <pd:inputBindings>
            <Input>
                <Values>
                    <xsl:value-of select="$Start/InvokeProcessInput/FixedData"/>
                </Values>
            </Input>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Map Data">
        <pd:type>com.tibco.plugin.mapper.MapperActivity</pd:type>
        <pd:resourceType>ae.activities.MapperActivity</pd:resourceType>
        <pd:x>322</pd:x>
        <pd:y>62</pd:y>
        <config>
            <element>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx:AcctDetails"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </element>
        </config>
        <pd:inputBindings>
            <xsl:variable name="var" select="$GetDataFormat/root/Rows/Format[1]"/>
            <root>
                <pfx:AcctDetails>
                    <pfx:RecordIndicator>
                        <xsl:value-of select="$var/RecordType"/>
                    </pfx:RecordIndicator>
                    <pfx:CustomerID>
                        <xsl:value-of select="$var/CustomerId"/>
                    </pfx:CustomerID>
                    <pfx:AcctNumber>
                        <xsl:value-of select="$var/AccountNumber"/>
                    </pfx:AcctNumber>
                    <pfx:TxnID>
                        <xsl:value-of select="$var/TransactionId"/>
                    </pfx:TxnID>
                    <xsl:if test="$var/TransactionType">
                        <pfx:TxnTypeCode>
                            <xsl:value-of select="$var/TransactionType"/>
                        </pfx:TxnTypeCode>
                    </xsl:if>
                    <pfx:TxnAmount>
                        <xsl:value-of select="$var/TransactionAmount"/>
                    </pfx:TxnAmount>
                    <pfx:Currency>
                        <xsl:value-of select="$var/CurrencyCode"/>
                    </pfx:Currency>
                    <pfx:TxnDate>
                        <xsl:value-of select="$var/TransactionDate"/>
                    </pfx:TxnDate>
                    <pfx:TxnTime>
                        <xsl:value-of select="$var/TransactionTime"/>
                    </pfx:TxnTime>
                    <xsl:if test="$var/BranchCode">
                        <pfx:BranchID>
                            <xsl:value-of select="$var/BranchCode"/>
                        </pfx:BranchID>
                    </xsl:if>
                    <xsl:if test="$var/ChannelCode">
                        <pfx:ChannelType>
                            <xsl:value-of select="$var/ChannelCode"/>
                        </pfx:ChannelType>
                    </xsl:if>
                    <xsl:if test="$var/TransactionStatus">
                        <pfx:StatusCode>
                            <xsl:value-of select="$var/TransactionStatus"/>
                        </pfx:StatusCode>
                    </xsl:if>
                    <pfx:CurrentBalance>
                        <xsl:value-of select="$var/AvailableBalance"/>
                    </pfx:CurrentBalance>
                    <pfx:ProcessDate>
                        <xsl:value-of select="$var/ProcessingDate"/>
                    </pfx:ProcessDate>
                    <pfx:AuthCode>
                        <xsl:value-of select="&quot;OAuth&quot;"/>
                    </pfx:AuthCode>
                    <pfx:FeeAmount>
                        <xsl:value-of select="0"/>
                    </pfx:FeeAmount>
                </pfx:AcctDetails>
            </root>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Render JSON">
        <pd:type>com.tibco.plugin.json.activities.JSONRenderActivity</pd:type>
        <pd:resourceType>ae.activities.JSONRenderActivity</pd:resourceType>
        <pd:x>498</pd:x>
        <pd:y>62</pd:y>
        <config>
            <SchemaType>xsdType</SchemaType>
            <BadgerfishCheck>false</BadgerfishCheck>
            <ActivityInputEditor>
                <xsd:element name="root">
                    <xsd:complexType>
                        <xsd:sequence>
                            <xsd:element ref="pfx:AcctDetails"/>
                        </xsd:sequence>
                    </xsd:complexType>
                </xsd:element>
            </ActivityInputEditor>
        </config>
        <pd:inputBindings>
            <ns:ActivityInputClass>
                <root>
                    <xsl:copy-of select="$Map-Data/root/pfx:AcctDetails"/>
                </root>
            </ns:ActivityInputClass>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>GetDataFormat</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>GetDataFormat</pd:from>
        <pd:to>Map Data</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Map Data</pd:from>
        <pd:to>Render JSON</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Render JSON</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>